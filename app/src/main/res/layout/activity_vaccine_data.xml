<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"



    android:background="#72c3ec"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context="gotz8.disease.VaccineData">



    <LinearLayout
        android:id="@+id/container_toolbar2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <include
            android:id="@+id/toolbarbacdata"
            layout="@layout/toolbarbacdata" />
    </LinearLayout>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Vaccine"
        android:id="@+id/textViewvir1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:textSize="20sp"
        android:textStyle="normal|bold"
        android:textColor="#ffffff"/>




    <ImageView

        android:id="@+id/imageView69"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_marginLeft="8dp"
        android:onClick="ClickVacData"
        android:layout_marginTop="7dp"


        android:background="@drawable/arrowb1"
        />


    <LinearLayout
        android:id="@+id/lin2"
        android:layout_width="390dp"

        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/corner_1"
        android:orientation="vertical"
        android:layout_below="@+id/container_toolbar2"
        android:weightSum="1">

        <TextView
            android:id="@+id/VacName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="22dp"
            android:text="Vaccine"
            android:textColor="#ffffff"
            android:textSize="24sp"
            android:textStyle="bold" />



        <TextView
            android:id="@+id/vacman"
            android:layout_width="320dp"
            android:layout_height="wrap_content"

            android:layout_below="@+id/VacName"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="12dp"

            android:text="company"
            android:textColor="#ffffff"

            android:textSize="22sp" />

        <TextView
            android:id="@+id/vactype"
            android:layout_width="320dp"
            android:layout_height="wrap_content"

            android:layout_below="@+id/vacman"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="20dp"
            android:text="type"

            android:textColor="#ffffff"

            android:textSize="22sp" />

        <TextView
            android:id="@+id/vacm1"
            android:layout_width="320dp"
            android:layout_height="wrap_content"

            android:layout_below="@+id/vactype"
            android:layout_marginBottom="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"

            android:text="description"
            android:textColor="#ffffff"

            android:textSize="22sp" />


    </LinearLayout>


</RelativeLayout>



