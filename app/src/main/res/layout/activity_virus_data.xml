<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#72c3ec"


    >
    <LinearLayout
        android:id="@+id/container_toolbar2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <include
            android:id="@+id/toolbarbacdata"
            layout="@layout/toolbarbacdata" />
    </LinearLayout>
    <ImageView

        android:id="@+id/imageView90"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="8dp"
        android:onClick="ClickVirusData"
        android:layout_marginLeft="11dp"
        android:scaleType="fitXY"
        android:background="@drawable/arrowb1"

        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/virus"
        android:id="@+id/textViewvir1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:textSize="24sp"
        android:textStyle="normal|bold"
        android:textColor="#ffffff"/>

    <TextView
        android:id="@+id/VirNameEng"
        android:layout_width="360dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:text="VIRUS NAME"
        android:layout_centerHorizontal="true"
        android:textColor="#ffffff"
        android:shadowColor="#000000"
        android:shadowDx="1.5"
        android:shadowDy="1.3"
        android:shadowRadius="3"
        android:textAllCaps="true"
        android:textSize="24sp"
        android:textStyle="bold"
        android:layout_below="@+id/container_toolbar2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="20dp"
        android:layout_marginStart="30dp" />



    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="12dp"
        android:layout_marginBottom="15dp"

        android:background="@drawable/corner_1"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginRight="10dp"
        android:layout_below="@+id/VirNameEng"
        android:layout_marginEnd="10dp">


            <TextView
                android:id="@+id/VirDT"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"

                android:layout_marginBottom="30dp"
                android:textColor="#ffffff"
                android:layout_marginLeft="30dp"
                android:layout_marginStart="21dp"
                android:layout_marginRight="15dp"
                android:layout_marginEnd="15dp"
                android:layout_marginTop="12dp"
                android:text="symptom"

                android:textSize="22sp" />


        </ScrollView>


</RelativeLayout>



