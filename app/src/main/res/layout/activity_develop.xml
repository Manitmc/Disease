<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_develop"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="gotz8.disease.Develop"
    android:background="#d0d0d0">
    <LinearLayout
        android:id="@+id/container_toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <include
            android:id="@+id/toolbardev"
            layout="@layout/toolbardev" />
    </LinearLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/dev"
        android:id="@+id/textView22"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:layout_gravity="center_horizontal"
        android:textSize="20sp"
        android:textStyle="normal|bold"
        android:textColor="#FFFFFF" />


    <ImageView

        android:id="@+id/imageView69"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_marginLeft="8dp"

        android:layout_marginTop="7dp"
        android:onClick="ClickDev"

        app:srcCompat="@drawable/arrowb1" />

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="false"
        android:layout_below="@+id/container_toolbar"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/box_dev"
        android:layout_alignParentRight="false">

        <ImageView
            android:id="@+id/imageView77"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:layout_marginTop="5dp"
            android:src="@drawable/rmutsb_logo_en" />

        <TextView
            android:text="@string/devcom"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/textView35"
            android:textSize="18sp"

            android:foregroundGravity="center_vertical|center_horizontal|center"
            android:gravity="center_vertical|center_horizontal|center"
            android:layout_marginTop="15dp"

            android:textStyle="bold" />

        <TextView
            android:text="@string/faculty"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/textView37"
            android:layout_marginTop="15dp"
            android:foregroundGravity="center_vertical|center_horizontal|center"
            android:gravity="center_vertical|center_horizontal|center"
            android:textSize="15sp"
            android:textColor="@color/colorPrimary"
            android:textStyle="normal|bold" />

        <TextView
            android:id="@+id/textView41"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:foregroundGravity="center_vertical|center_horizontal|center"
            android:gravity="center_vertical|center_horizontal|center"
            android:text="@string/Major"
            android:textColor="#f27130"
            android:textSize="15sp"

            android:textStyle="normal|bold" />

        <TextView
            android:text="@string/Devname1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/textView42"
            android:layout_marginTop="15dp"
            android:foregroundGravity="center_vertical|center_horizontal|center"
            android:gravity="center_vertical|center_horizontal|center"
            android:textSize="12sp"
            />

        <TextView
            android:text="@string/Devmail1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/textViewEmailnut"
            android:layout_marginTop="3dp"
            android:foregroundGravity="center_vertical|center_horizontal|center"
            android:gravity="center_vertical|center_horizontal|center"
            android:textSize="12sp"
            />

        <TextView
            android:text="@string/Devname2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="25dp"
            android:foregroundGravity="center_vertical|center_horizontal|center"
            android:gravity="center_vertical|center_horizontal|center"
            android:textSize="12sp"
            android:id="@+id/textView43"

            />

        <TextView
            android:text="@string/Devmail2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/textViewEmailKampoo"
            android:layout_marginTop="3dp"
            android:foregroundGravity="center_vertical|center_horizontal|center"
            android:gravity="center_vertical|center_horizontal|center"
            android:textSize="12sp"
            android:layout_marginBottom="20dp"
            />
    </LinearLayout>

    <TextView
        android:text="@string/devby"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView33"
        android:foregroundGravity="center_vertical|center_horizontal|center"
        android:gravity="center_vertical|center_horizontal"
        android:textSize="18sp"
        android:textStyle="normal|bold"
        android:layout_marginTop="18dp"
        android:layout_below="@+id/container_toolbar"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
         />

</RelativeLayout>
